<%# # Form_for it takes a model object, looks at its attributes, and    
# uses those attributes to pre-populate the form's fields. %>
  <%= simple_form_for(@store) do |f| %>
    <%= f.error_notification %>
    <div class="row">
      <div class="form-inputs">
        <%= f.input :name, label_html: { class: 'active' } %>
        <%= f.input :street, label_html: { class: 'active' }%>
        <%= f.input :city, label_html: { class: 'active' } %>
        <%= f.input :state, label_html: { class: 'active' } %>
        <%= f.input :zip, label_html: { class: 'active' } %>
        <%= f.input :phone, label_html: { class: 'active' } %>
        <%= f.input :active %>
      </div>
    </div>
    <div class="row">
      <div class="form-actions col s8">
        <%= f.button :submit %>
      </div>
    </div>
  <% end %>